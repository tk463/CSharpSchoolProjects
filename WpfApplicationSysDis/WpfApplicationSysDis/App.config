<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="WSSoapPortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/WebApplicationForWS/WSSoap"
                binding="basicHttpBinding" bindingConfiguration="WSSoapPortBinding"
                contract="ServiceReferenceWS.WSSoap" name="WSSoapPort" />
        </client>
    </system.serviceModel>
</configuration>